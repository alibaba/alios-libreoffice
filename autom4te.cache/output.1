test_font_map()
{
    FONT="$1" ; shift
    { $as_echo "$as_me:${as_lineno-$LINENO}: checking font mapping for '$FONT'" >&5
$as_echo_n "checking font mapping for '$FONT'... " >&6; }
    FONTFILE="$(basename `$FCMATCH -f '%{file}' "$FONT"`)"
    FONTFILE_LOWER="$(echo $FONTFILE | $AWK '{print tolower($0)}')"

    TESTEXPR="'${FONTFILE_LOWER}' = '$(echo $FONT | $AWK '{print tolower($0)}').ttf'"
    while test "$#" -ge 1 ; do
        MAPPING="$(echo $1 | $AWK '{print tolower($0)}')"; shift
        TESTEXPR="${TESTEXPR} -o '${FONTFILE_LOWER}' = '$MAPPING-regular.ttf'"
    done
    if test $TESTEXPR
    then
        { $as_echo "$as_me:${as_lineno-$LINENO}: result: ok" >&5
$as_echo "ok" >&6; }
    else
        { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: unknown ($FONTFILE)" >&5
$as_echo "$as_me: WARNING: unknown ($FONTFILE)" >&2;}
        add_warning "unknown ($FONTFILE)"
        TEST_FONTS_MISSING=1
    fi
}

